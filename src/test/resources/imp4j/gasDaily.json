{
  //表名
  "tableName": "gas_daily",
  //数据从第几行开始，第一行为1
  "startRow": "2",
  //校验过程中两个值，error：错误直接停止插入，warn：忽略错误插入有效行
  "error": "error",
  //列配置
  "columns": [
    //列名，取值(带{}为表达式)，校验(正则表达式，没有留空)，格式(没有留空)
    //ID字段取第一列
    ["id","{$1}","",""],
    //price取第一列和第二列只和
    ["price","{$2+$3}","",""],
    //create_time 插入当前时间
    ["create_time","now()","",""]
  ]
}